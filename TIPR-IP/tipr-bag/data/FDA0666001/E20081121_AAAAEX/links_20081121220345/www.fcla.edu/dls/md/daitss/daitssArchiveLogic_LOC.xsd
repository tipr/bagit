  			<xsd:element name="ID" type="ArchiveLogicIdType"></xsd:element>
  			<xsd:element name="ACCOUNT_PROJECT" type="AccountProjectIdType"></xsd:element>
  			<xsd:element name="START_DATE" type="DateType"></xsd:element>
  			<xsd:element name="END_DATE" type="DateType" minOccurs="0"></xsd:element>
  			<xsd:element name="MEDIA_TYPE" type="MediaType"></xsd:element>
  			<xsd:element name="PRES_LEVEL" type="LogicalPresLevelType"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="ArchiveLogicIdType">
		<xsd:restriction base="VarChar32Type"></xsd:restriction>
	</xsd:simpleType>
	
</xsd:schema>

